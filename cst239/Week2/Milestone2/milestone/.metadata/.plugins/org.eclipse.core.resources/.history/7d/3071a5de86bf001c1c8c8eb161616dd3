package MainApp;

import java.util.List;

public class ProductListManager
{
	private List<String> productStringList;
	
	public void AddItemToCart(String item) {productStringList.add(item);}
	
	public void RemoveItemFromCart(String item){productStringList.remove(item);}
	
	public List<String> GetCartList() {return productStringList;}
	
	public void SetCartList(List<String> newCartList) {productStringList = newCartList;}
	
	
	
	public void AddItemToProducts(Product item) {cartProducts.add(item);}
	
	public void RemoveItemFromProducts(Product item){cartProducts.remove(item);}
	
	public List<Product> GetCartProducts() {return cartProducts;}
	
	public void SetCartProducts(List<Product> newCartProducts) {cartProducts = newCartProducts;}
}
