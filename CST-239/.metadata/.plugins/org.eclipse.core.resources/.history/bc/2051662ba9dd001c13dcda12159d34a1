package test;

import org.junit.Test;

public class CalculatorTest
{
	@Test
	public int TestAdd(int a, int b) 
	{
		return a + b;
	}
	@Test
	public int TestSubtract(int a, int b) 
	{
		return a - b;
	}
	@Test
	public int TestMultiply(int a, int b) 
	{
		return a * b;
	}
	@Test
	public int TestDivide(int a, int b) 
	{
		return a / b;
	}
}
